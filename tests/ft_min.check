#include <check.h>
#include "libft.h"

#ifndef INT_MAX
# define INT_MAX		INT32_MAX
#endif

#ifndef INT_MIN
# define INT_MIN		INT32_MIN
#endif


#test ft_min_test
	fail_unless(ft_min(INT_MAX, 2) == 2, "INT_MAX vs 2 failed");
	fail_unless(ft_min(INT_MAX, INT_MAX) == INT_MAX, "INT_MAX vs INT_MAX failed");
    fail_unless(ft_min(INT_MIN, INT_MAX) == INT_MIN, "INT_MIN vs INT_MAX failed");
    fail_unless(ft_min(-3, 2) == -3, "-3 vs 2 failed");
    fail_unless(ft_min(0, -2) == -2, "0 vs -2 failed");
    fail_unless(ft_min(-3, -2) == -3, "-3 vs -2 failed");
